extends ../layouts/index


block variables
  include ../../data/data.pug
  - const title = "Mars"


block content

  include ../../blocks/components/breadcrumb/breadcrumb.pug
  include ../../blocks/modules/news-details/news-details.pug

  .container
    .page 
     .page__breadcrumbs
      +breadcrumb([
        'Главная',
        'Новости',
        'Отчет о проведении НТМ Международная студенческая конференция «Языки мира и культура»'
      ])
     .page__title
        h1 Новости
     .page__content
        +news-details()

block scripts
  script(src="/js/vendor.js") 
  script(src="/js/common.js") 
 
